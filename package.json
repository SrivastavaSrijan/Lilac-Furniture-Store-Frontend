{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "devs": "source $HOME/.nvm/nvm.sh; nvm use;nvm use && clear && next dev -p 7777",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "codegen": "graphql-codegen --config codegen.yml",
    "postcodegen": "next lint --fix; node post-codegen.js  && next lint --fix",
    "watch:graphql": "chokidar 'src/lib/graphql/**/*.graphql' -c 'npm run codegen'",
    "dev": "concurrently \"yarn devs\" \"yarn watch:graphql\""
  },
  "dependencies": {
    "@apollo/client": "^3.8.6",
    "@emotion/cache": "^11.11.0",
