    "@types/lodash": "^4.14.200",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-swipeable-views": "^0.13.4",
    "@types/react-swipeable-views-utils": "^0.13.6",
    "autoprefixer": "^10.0.1",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-nextjs-nos": "^1.0.2",
    "graphql-codegen-apollo-next-ssr": "^1.7.4",
