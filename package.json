    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.16",
    "@mui/material": "^5.14.16",
    "apollo-upload-client": "^18.0.1",
    "deepmerge": "^4.3.1",
    "framer-motion": "^10.16.4",
    "glob": "^10.3.10",
    "graphql": "^16.8.1",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.2",
    "next": "14.0.1",
    "next-cloudinary": "^5.3.0",
    "next-seo": "^6.4.0",
    "react": "^18",
    "react-dom": "^18",
    "react-swipeable-views": "^0.14.0",
