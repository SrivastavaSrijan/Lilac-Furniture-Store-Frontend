{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "devs": "source $HOME/.nvm/nvm.sh; nvm use;nvm use && clear && next dev -p 80  & local-ssl-proxy --key local.lilac.srijansrivastava.com-key.pem --cert local.lilac.srijansrivastava.com.pem --source 443 --target 80",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "codegen": "graphql-codegen --config codegen.yml",
    "postcodegen": "next lint --fix; node post-codegen.js  && next lint --fix && clear",
    "watch:graphql": "chokidar 'src/lib/graphql/**/*.graphql' -c 'npm run codegen'",
    "dev": "concurrently \"yarn devs\" \"yarn watch:graphql\""
  },
